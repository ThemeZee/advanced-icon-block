{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "themezee/advanced-icon-block",
	"version": "0.1.0",
	"title": "Advanced Icon",
	"category": "media",
	"description": "Display a SVG icon.",
	"textdomain": "advanced-icon-block",
	"attributes": {
		"iconName": {
			"type": "string",
			"default": "pencil"
		},
		"iconLibrary": {
			"type": "string",
			"default": "wordpress"
		},
		"iconWidth": {
			"type": "string",
			"default": "36px"
		},
		"iconHeight": {
			"type": "string",
			"default": "36px"
		},
		"justification": {
			"type": "string"
		},
		"rotate": {
			"type": "number",
			"default": 0
		},
		"flipHorizontal": {
			"type": "boolean"
		},
		"flipVertical": {
			"type": "boolean"
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "href"
		},
		"linkTarget": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "target"
		},
		"rel": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "rel"
		}
	},
	"supports": {
		"align": true,
		"html": false,
		"color": {
			"__experimentalSkipSerialization": true,
			"gradients": true,
			"__experimentalDefaultControls": {
				"text": true,
				"background": true
			}
		},
		"spacing": {
			"__experimentalSkipSerialization": true,
			"margin": true,
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": true
			}
		},
		"__experimentalBorder": {
			"color": true,
			"style": true,
			"width": true,
			"radius": true,
			"__experimentalSkipSerialization": true,
			"__experimentalDefaultControls": {
				"radius": true
			}
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
